<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="leasecity.repo.leasecityAdminRepo">
 	
 	<select id="getSelectLeasecityAdmin" parameterType="Map"
 						 resultType="leasecityAdmin">
 		SELECT *
 		FROM leasecity_admin
 		WHERE admin_id = #{adminId} AND admin_pass = #{adminPass}
 	</select>
 	
 	<update id="updateLeasecityAdmin" parameterType="leasecityAdmin">
 		UPDATE leasecity_admin SET
 		admin_id = #{adminId}, admin_pass = #{adminPass} 		
 	</update>
 
 </mapper>